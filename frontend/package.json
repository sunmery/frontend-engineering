{
  "name": "frontend",
  "private": true,
  "version": "1.0.1",
  "type": "module",
  "lint-staged": {
    "*.{ts,tsx,js,jsx,cjs,mjs,md}": "eslint --fix",
    "*.{css,scss,sass}": "stylelint --fix",
    "*.{ts,tsx,js,jsx,cjs,mjs}": "prettier --write"
  },
  "scripts": {
    "prepare": "cd .. && husky frontend/.husky",
    "prettier:check": "prettier --check src/**/**{.cjs,.mjs,.ts,.tsx,.html,.md}",
    "prettier:write": "prettier --write src/**/**{.cjs,.mjs,.ts,.tsx,.html,.md}",
    "lint": "lint-staged",
    "oxlint": "oxlint",
    "eslint": "ESLINT_USE_FLAT_CONFIG=false eslint -c .eslintrc.yml **/*.{ts,tsx,md}",
    "eslint:fix": "eslint --cache --max-warnings 0 src/**/*{.ts,.tsx} --fix",
    "stylelint": "stylelint **/**.{css,scss,sass}",
    "stylelint:fix": "stylelint **/**.{css,scss,sass} --fix",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 8080",
    "tauri": "tauri",
    "commitlint": "commitlint --edit",
    "release": "standard-version"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.18",
    "@mui/joy": "5.0.0-beta.36",
    "@mui/material": "^5.15.18",
    "@tauri-apps/api": "^1.5.6",
    "@vitejs/plugin-react": "^4.3.0",
    "install": "^0.13.0",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1",
    "sort-by": "^1.2.0",
    "vite": "^5.2.11"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@babel/eslint-parser": "^7.24.6",
    "@babel/preset-react": "^7.24.6",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/prompt-cli": "^19.3.1",
    "@eslint/js": "^8.57.0",
    "@tauri-apps/cli": "^1.5.14",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "commitlint": "^19.3.0",
    "eslint": "^8.57.0",
    "eslint-config-alloy": "^5.1.2",
    "eslint-plugin-jsdoc": "^48.2.6",
    "eslint-plugin-markdown": "^4.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.4",
    "oxlint": "^0.2.18",
    "prettier": "3.2.5",
    "standard-version": "^9.5.0",
    "stylelint": "^16.6.0",
    "stylelint-config-recess-order": "^5.0.1",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "typescript": "^5.4.5"
  }
}
