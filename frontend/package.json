{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "lint-staged": {
    "*.{ts,tsx,js,jsx,cjs,mjs,md}": "eslint --fix",
    "*.{css,scss,sass}": "stylelint --fix",
    "*.{ts,tsx,js,jsx,cjs,mjs}": "prettier --write"
  },
  "scripts": {
    "prepare": "cd .. && husky ./frontend/.husky || true",
    "prettier:check": "prettier --check src/**/**{.cjs,.mjs,.ts,.tsx,.html,.md}",
    "prettier:write": "prettier --write src/**/**{.cjs,.mjs,.ts,.tsx,.html,.md}",
    "lint": "lint-staged",
    "oxlint": "oxlint",
    "eslint": "ESLINT_USE_FLAT_CONFIG=false eslint -c .eslintrc.yml **/*.{ts,tsx,md}",
    "eslint:fix": "eslint --cache --max-warnings 0 src/**/*{.ts,.tsx} --fix",
    "stylelint": "stylelint **/**.{css,scss,sass}",
    "stylelint:fix": "stylelint **/**.{css,scss,sass} --fix",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 8080",
    "tauri": "tauri",
    "commitlint": "commitlint --edit"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/joy": "5.0.0-beta.36",
    "@tauri-apps/api": "^1",
    "vite": "^5.1.6",
    "react": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/eslint-parser": "^7.23.10",
    "@babel/preset-react": "^7.23.3",
    "@commitlint/cli": "^19.2.0",
    "@commitlint/config-conventional": "^19.1.0",
    "@commitlint/prompt-cli": "^19.2.0",
    "@eslint/js": "^8.57.0",
    "@tauri-apps/cli": "^1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "commitlint": "^19.2.0",
    "eslint": "^8.57.0",
    "eslint-config-alloy": "^5.1.2",
    "eslint-plugin-jsdoc": "^48.2.1",
    "eslint-plugin-markdown": "^4.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "oxlint": "^0.2.13",
    "prettier": "3.2.5",
    "stylelint": "^16.2.1",
    "stylelint-config-recess-order": "^5.0.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "typescript": "^5.4.2"
  }
}
